# A sample Gemfile
source "https://rubygems.org"
ruby "2.1.2" # see https://devcenter.heroku.com/articles/ruby-versions

gem 'sinatra'
gem 'haml'
gem 'sass'
gem 'pony'

group :development do
  gem 'sinatra-contrib'
  gem 'pry'
  gem 'pry-remote'
  gem 'pry-stack_explorer'
  gem 'pry-byebug', '1.3.3' # this version is necessary for pry-remote to work see http://bit.ly/1JegLMF
end

